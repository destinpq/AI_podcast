{
  "name": "ai-podcast",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev:frontend": "next dev -p 7777",
    "dev:backend": "cd backend && npm run start:dev",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "build:frontend": "NEXT_DISABLE_ESLINT=1 next build --no-lint",
    "build:backend": "cd backend && npm install && npm run build",
    "build": "npm install && npm run build:frontend && npm run build:backend",
    "start:frontend": "next start -p 7777",
    "start:backend": "cd backend && npm run start:prod",
    "start": "concurrently \"npm:start:frontend\" \"npm:start:backend\"",
    "lint": "next lint",
    "deploy": "vercel deploy --prod"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/material-nextjs": "^6.4.3",
    "axios": "^1.8.4",
    "firebase": "^11.5.0",
    "framer-motion": "^12.6.0",
    "google-trends-api": "^4.9.2",
    "googleapis": "^148.0.0",
    "jspdf": "^3.0.1",
    "next": "15.2.4",
    "openai": "^4.92.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-force-graph": "^1.47.6",
    "react-force-graph-2d": "^1.27.1",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
