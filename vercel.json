{
  "version": 2,
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "buildCommand": "yarn build:no-lint",
  "devCommand": "next dev",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_DISABLE_ESLINT": "1"
  }
} 